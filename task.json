{
    "family": "My-fargate",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "my-container",
            "image": "207797650028.dkr.ecr.ap-south-1.amazonaws.com/naveen-repo:18",
            "portMappings": [
                {
                    "containerPort": 5000 ,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            
            ],
            "ulimits": [
                {
                   "name": "nofile",
                   "softLimit": 256
                }
            ]
        }
    ],

    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}

